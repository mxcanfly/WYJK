@model PagedResult<InsuranceListViewModel>
@{
    Layout = "~/Views/User/_Layout_user.cshtml";
    ViewBag.Title = "我的借款";
}
@section Header{
    <script>
        $(function () {
            var menus = $("#leftMenu li");
            menus.removeClass("active");

            $("#leftMenu #userLoanItem").addClass("active");
            
        });

    </script>
}
<div class="user-right-w pull-right am-margin-bottom-lg am-margin-left-lg">
            <div class="panel panel-default bg-lightgrey am-margin-bottom-0">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <form action="" method="post" class="form-inline">
                            <div class="clearfix">
                                <div class="form-group pull-left">
                                    <label for="" class="am-padding-right-sm">审核状态</label>
                                    <select name="" class="form-control">
                                        <option value="">全部</option>
                                        <option value="">待审核</option>
                                        <option value="">未通过</option>
                                        <option value="">已通过</option>
                                    </select>
                                </div>
                                <div class="pull-right">
                                    <button type="button" class="btn btn-info bg-lightblue">查询</button>
                                </div>
                            </div>
                        </form>
                    </h3>
                </div>
                <!--列表-->
                <div class="insurance-list-w">
                    <ul class="list-unstyled am-margin-0">
                        @foreach (var item in Model.Items)
                        {
                            <li class="row bg-white">
                                <div class="col-lg-12">
                                    <dl class="am-margin-0">
                                        <dd class="row am-padding-vertical-sm">
                                            <div class="col-lg-4">
                                                总循环额度：500000
                                            </div>
                                            <div class="col-lg-4">
                                                已用循环额度：200000
                                            </div>
                                            <div class="col-lg-4">
                                                可用循环额度：50000
                                            </div>
                                        </dd>
                                        <dd class="row am-padding-vertical-sm">
                                            <div class="col-lg-4">
                                                状态：<span class="text-primary">已通过</span>
                                            </div>
                                            <div class="col-lg-4">
                                                提交时间：2016-01-18
                                            </div>
                                            <div class="col-lg-4">
                                                审核时间：2016-04-12
                                            </div>
                                        </dd>
                                        <dd class="row am-padding-vertical-sm">
                                            <div class="col-lg-4">
                                                滞纳金
                                            </div>
                                            <div class="col-lg-4">
                                                <a href="loan-payback-s1.html" class="orange">我要还款</a>
                                            </div>
                                            <div class="col-lg-4">
                                                <a href="user-myLoan-detail.html" class="orange">查看详情</a>
                                            </div>
                                        </dd>
                                    </dl>
                                </div>
                            </li>
                        }
                        
                    </ul>
                </div>
                <!--分页-->
                <div class="text-center">
                    <nav>
                        <ul class="pagination">
                            <li>
                                <a href="#" aria-label="Previous">
                                    <span aria-hidden="true">&laquo;</span>
                                </a>
                            </li>
                            <li class="active"><a href="#">1</a></li>
                            <li><a href="#">2</a></li>
                            <li><a href="#">3</a></li>
                            <li><a href="#">4</a></li>
                            <li><a href="#">5</a></li>
                            <li>
                                <a href="#" aria-label="Next">
                                    <span aria-hidden="true">&raquo;</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>